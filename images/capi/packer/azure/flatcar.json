{
  "ansible_extra_vars": "ansible_python_interpreter=/opt/bin/python",
  "build_name": "flatcar",
  "image_url": "https://flatcarimages.blob.core.windows.net/flatcarvmdk/flatcar_production_azure_image.vhd",
  "distribution": "Flatcar Linux",
  "distribution_release": "{{ env `FLATCAR_CHANNEL` }}",
  "distribution_version": "{{env  `FLATCAR_VERSION` }}",
  "containerd_cri_socket": "/run/docker/libcontainerd/docker-containerd.sock",
  "crictl_source_type": "http",
  "containerd_version": "1.4.3",
  "containerd_sha256": "2697a342e3477c211ab48313e259fd7e32ad1f5ded19320e6a559f50a82bff3d",
  "goss_entry_file": "goss/goss-flatcar.yaml",
  "kubernetes_cni_source_type": "http",
  "kubernetes_source_type": "http",
  "python_path": "/opt/bin/builder-env/site-packages",
  "___root_device_name": "/dev/xvda",
  "ssh_username": "core",
  "systemd_prefix": "/etc/systemd",
  "sysusr_prefix": "/opt",
  "sysusrlocal_prefix": "/opt"
}


